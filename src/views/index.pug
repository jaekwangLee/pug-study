extends ./layout/page.layout.pug

block append head 
    link(rel="stylesheet", href="/src/sass/home.css")
    script(type='text/javascript', src='/src/js/posts.js')
    link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.css")
    script(src="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.js" defer)
    script(type='text/javascript', src='/src/js/swiper.js' defer)

block header
    include ./component/header.pug
    
block content
    include ./component/bsHeader.pug
    section(class="statics")
        div(class="statics-inner-container")
            div(class="statics-section-title")
                h1 압도적 1등, 카모아
                p 따라올 수 없는 압도적 1등
                p 이제부터 렌트카 예약은 카모아입니다.
                p (전국 차량 공유 플랫폼 중 차량 보유수 최다)

            - const statics = [{ label: '누적예약', value: '608,246' },{ label: '사용자 리뷰', value: '81,192' },{ label: '예약 가능 차량', value: '52,927' },{ label: '전국 렌트카 지점', value: '1,495' }];
            div(class="statics-section-values")
                each val in statics
                    div(class="static-option")
                        h3(class="statics-label")= val.value
                        p(class="statics-value")= val.label
    
    section
        div 
            p(onclick="getPosts()") 클릭!!
        div(class="swiper index-slide-swiper")
            div(class="swiper-wrapper")
                div(class="swiper-slide")
                    div
                        img(src="data:image/svg+xml;base64,", alt="카모아 로고")
                        p Slide1
                div(class="swiper-slide")
                    div
                        img(src="data:image/svg+xml;base64,", alt="카모아 로고")
                        p Slide2
                div(class="swiper-slide")
                    div
                        img(src="data:image/svg+xml;base64,", alt="카모아 로고")
                        p Slide3
                div(class="swiper-slide")
                    div
                        img(src="data:image/svg+xml;base64,", alt="카모아 로고")
                        p Slide4
                div(class="swiper-slide")
                    div
                        img(src="data:image/svg+xml;base64,", alt="카모아 로고")
                        p Slide5
            div(class="swiper-pagination")
            div(class="swiper-button-prev")
            div(class="swiper-button-next")
